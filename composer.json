{
    "name": "vaninsky/yii2-rlock",
    "description": "Extend models: lockable by redis & translatable",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "vsv",
            "email": "vaninsky@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-redis": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "vaninsky\\rlock\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "vaninsky\\rlock\\Bootstrap",
        "branch-alias": {
            "dev-master": "0.2.3-dev"
        }
    }
}
